<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      href="https://fonts.googleapis.com/css2?family=Jost:wght@400;700&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Jost:wght@400;700&family=Lobster&display=swap"
      rel="stylesheet"
    />
    <link rel="stylesheet" href="./styles.css" />

    <title>CraftStudio</title>
  </head>
  <body>
    
    {{>header}}
    {{>navigation}}

    <section class="hero">
      <div class="carousel">
        {{#each products}}
        <div class="owl-carousel owl-theme">
          <div class="carousel-item" style="background-image: url({{img}});">
            <p class="hero-sub">{{category}}</p>
            <h1 class="hero-title">{{productName}}</h1>

           {{#if isOnSale}}
           <p class="products_sale">{{salePercentage}}% sale!</p>
           <p style="text-align: center;">
          <span
            class="products_price products_price-old"
            style="text-decoration:line-through; color:grey;"
          >${{price}}</span>
          <span class="products_price">${{#priceCounter
              price
              salePercentage
            }}{{/priceCounter}}</span></p>
          {{else}}
          <p class="products_price">${{price}}</p>
         {{/if}}
            <button type="button" class="carousel-item_btn">add</button>
          </div>
         {{/each}}
        </div>
    </section>

    <section class="new-dishes container">
      <h2 class="new-dishes_title">New Dishes</h2>
      <ul class="new-dishes_btn-list list">
        <li class="new-dishes_btn-list-item">
          <button class="new-dishes_btn">Burger</button>
        </li>
        <li class="new-dishes_btn-list-item">
          <button class="new-dishes_btn">Pizza</button>
        </li>
        <li class="new-dishes_btn-list-item">
          <button class="new-dishes_btn">Sandvich</button>
        </li>
        <li class="new-dishes_btn-list-item">
          <button class="new-dishes_btn">Fries</button>
        </li>
        <li class="new-dishes_btn-list-item">
          <button class="new-dishes_btn">Other</button>
        </li>
      </ul>

