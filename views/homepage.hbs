          <ul class="deals--right_btn-list list">
            <li class="deals--right_btn-list-item">
              <button class="deals--right_btn">All</button>
            </li>
            <li class="deals--right_btn-list-item">
              <button class="deals--right_btn">Burgers</button>
            </li>
            <li class="deals--right_btn-list-item">
              <button class="deals--right_btn">Salad</button>
            </li>
            <li class="deals--right_btn-list-item">
              <button class="deals--right_btn">Sandwich</button>
            </li>
            <li class="deals--right_btn-list-item">
              <button class="deals--right_btn">Fries</button>
            </li>
          </ul>
        </div>

        <ul class="deals--right_card-list list">
          {{#each onSale}}
            <li class="deals--right_card-item">
            <div class="deals--right_img" style="background-image: url({{img}});"></div>
            <div>
              <h3 class="deals--right-title">{{productName}}</h3>
              <p class="deals--right-price">{{price}} $</p>
              <p>{{isOnSale}} {{salePercentage}}% off  </p>
            </div>
          </li>
          {{/each}}
        </ul>
      </div>
