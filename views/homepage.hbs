    <section class="categories container">
      <h3 class="categories_title">Shop By Categories</h3>
      <ul class="categories_list list">
        <li class="categories_item">
          <div class="categories_img">
            <button class="categories_btn">
              Spageti<span class="categories_btn--items">(200 items)</span>
            </button>
          </div>
        </li>
        <li class="categories_item">
          <div class="categories_img">
            <button class="categories_btn">
              Pizza<span class="categories_btn--items">(200 items)</span>
            </button>
          </div>
        </li>
        <li class="categories_item">
          <div class="categories_img">
            <button class="categories_btn">
              Fries
              <span class="categories_btn--items">(200 items)</span>
            </button>
          </div>
        </li>
        <li class="categories_item">
          <div class="categories_img">
            <button class="categories_btn">
              Burgers
              <span class="categories_btn--items">(200 items)</span>
            </button>
          </div>
        </li>
      </ul>
    </section>

    <section class="best-seller container">
      <div class="best-seller_header">
        <h3 class="best-seller_title">Best Seller</h3>
        <ul class="best-seller_btn-list list">
          <li class="best-seller_btn-list-item">
            <button class="best-seller_btn">All</button>
          </li>
          <li class="best-seller_btn-list-item">
            <button class="best-seller_btn">Burger</button>
          </li>
          <li class="best-seller_btn-list-item">
            <button class="best-seller_btn">Pizza</button>
          </li>
          <li class="best-seller_btn-list-item">
            <button class="best-seller_btn">Sandwich</button>
          </li>
          <li class="best-seller_btn-list-item">
            <button class="best-seller_btn">Salad</button>
          </li>
        </ul>
      </div>

      <ul class="best-seller_card-list list">
           {{#each bestSellers}}
          <li class="best-seller_card-item">
          <img src={{img}} class="best-seller_img" alt="product-img">
          <h3 class="best-seller_card-title">{{productName}}</h3>
          <p class="best-seller_card-price">{{price}}$</p>
          <p>{{bestSeller}}</p>
        </li>
          {{/each}}
      </ul>
    </section>

    <section class="reviews container">
      <h3 class="reviews_title">What people say?</h3>

